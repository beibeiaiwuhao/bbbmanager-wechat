@import "../single-item/single-item-tpl.wxss";

.container {
  background-color: #efeff4;
  width: 100%;
  height: 100%;
  flex-direction: column;
  display: flex;
  align-items: center;
  min-height: 100vh;
}

.category-view {
  width: 100%;
  margin: 20rpx 0rpx 20rpx 18rpx;
}

.item-view {
  float: left;
  margin-bottom: 20rpx;
  padding: 0 20rpx;
}

.item-view image {
  width: 200rpx;
  height: 270rpx;
}
